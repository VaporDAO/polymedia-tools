{
    "private": false,
    "version": "0.0.3",
    "name": "@polymedia/commando",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://github.com/juzybits/polymedia-commando",
    "description": "Command line tools for Sui (airdrops, data, and more)",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-commando"
    },
    "keywords": [
        "polymedia",
        "sui",
        "commando"
    ],
    "type": "module",
    "scripts": {
        "aggregate_nfts": "pnpm build && node dist/esm/holderfinder/aggregate_nfts.js",
        "bulksender": "pnpm build && node dist/esm/bulksender/bulksender.js",
        "find_coin_balances": "pnpm build && node dist/esm/find_coin_balances.js",
        "find_coin_holders": "pnpm build && node dist/esm/find_coin_holders.js",
        "find_last_txn": "pnpm build && node dist/esm/find_last_txn.js",
        "find_nft_holders": "pnpm build && node dist/esm/holderfinder/find_nft_holders.js",
        "find_nfts": "pnpm build && node dist/esm/holderfinder/find_nfts.js",
        "test_endpoints": "pnpm build && node dist/esm/test_endpoints.js",
        "transform_balances_to_csv": "pnpm build && node dist/esm/transform_balances_to_csv.js",
        "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@mysten/sui.js": "^0.48.1",
        "graphql": "^16.8.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "typescript": "^5.3.3"
    },
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        "source": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
    }
}
